{
  "name": "enea/laravel-authorization",
  "description": "Package to manage the permissions in a laravel application",
  "type": "package",
  "authors": [
    {
      "name": "enea dhack",
      "email": "enea.so@live.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/contracts": "5.5.*|5.6.*",
    "illuminate/database": "5.5.*|5.6.*",
    "illuminate/support": "5.5.*|5.6.*",
    "symfony/http-kernel": "^3.2|^4.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5|^7.0",
    "orchestra/testbench": "^3.4",
    "orchestra/database": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Enea\\Authorization\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enea\\Authorization\\Tests\\": "tests"
    }
  }
}
